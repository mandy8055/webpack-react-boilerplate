{
  "name": "webpack-react-boilerplate",
  "version": "1.0.0",
  "description": "It is a boilerplate repo which contains webpack 5 configured along with husky, eslint, prettier and jest for react applications",
  "scripts": {
    "check-deps": "check-dependencies",
    "prebuild": "rimraf ./dist",
    "build": "pnpm prebuild && pnpm check-deps && cross-env NODE_ENV=production webpack --config webpack/webpack.prod.js",
    "build-dev": "pnpm check-deps && cross-env NODE_ENV=development webpack --config webpack/webpack.dev.js",
    "start": "pnpm check-deps && cross-env SERVE=true NODE_ENV=development webpack serve --config webpack/webpack.dev.js",
    "test": "jest",
    "prepare": "husky install",
    "lint": "eslint .",
    "lint:fix": "eslint --fix ."
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mandy8055/webpack-react-boilerplate.git"
  },
  "author": "mandy8055",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/mandy8055/webpack-react-boilerplate/issues"
  },
  "homepage": "https://github.com/mandy8055/webpack-react-boilerplate#readme",
  "devDependencies": {
    "@babel/core": "^7.18.9",
    "@babel/preset-env": "^7.18.9",
    "@babel/preset-react": "^7.18.6",
    "@pmmmwh/react-refresh-webpack-plugin": "^0.5.7",
    "@testing-library/jest-dom": "^5.17.0",
    "@testing-library/react": "^14.0.0",
    "babel-jest": "^29.6.2",
    "babel-loader": "^8.2.5",
    "check-dependencies": "^1.1.0",
    "clean-webpack-plugin": "^4.0.0",
    "cross-env": "^7.0.3",
    "css-loader": "^6.7.1",
    "eslint": "^8.20.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jsx-a11y": "^6.6.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.33.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-webpack-plugin": "^3.2.0",
    "html-webpack-plugin": "^5.5.0",
    "husky": "^8.0.3",
    "jest": "^29.6.2",
    "jest-cli": "^28.1.3",
    "jest-environment-jsdom": "^29.6.2",
    "mini-css-extract-plugin": "^2.6.1",
    "postcss": "^8.4.14",
    "postcss-loader": "^7.0.1",
    "postcss-preset-env": "^7.7.2",
    "prettier": "^2.7.1",
    "react-refresh": "^0.14.0",
    "react-test-renderer": "^18.2.0",
    "rimraf": "^3.0.2",
    "sass": "^1.53.0",
    "sass-loader": "^13.0.2",
    "webpack": "^5.73.0",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^4.15.1",
    "webpack-merge": "^5.9.0"
  },
  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  }
}
